{
    "name": "stubbles/image",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Simple image handling support.",
    "require": {
        "php": ">=5.4.0",
        "ext-gd": "*",
        "stubbles/core": "~5.0"
    },
    "suggest": {
        "stubbles/webapp-core": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "stubbles/webapp-core": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\img\\": "src/main/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
